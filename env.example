# Aliquot QA Testing Configuration
# Copy this file to .env and fill in your actual values

# QA Environment URL
ALIQUOT_BASE_URL_QA=https://qa.aliquot.live/

# Authentication Credentials
ALIQUOT_USERNAME_QA=your_username@example.com
ALIQUOT_PASSWORD_QA=your_password

# Invalid credentials for testing
ALIQUOT_INVALID_EMAIL=invalid@example.com
ALIQUOT_INVALID_PASSWORD=wrongpassword

# Test Configuration
HEADLESS=false
CI=false

# Allure Reporting
ALLURE_RESULTS_DIR=allure-results
ALLURE_REPORT_DIR=allure-report
