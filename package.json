{
  "name": "aqua-regression-pw",
  "version": "1.0.0",
  "description": "Aqua Regression Testing Suite - Comprehensive regression testing for Aqua application",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "playwright test tests/regression/functional/My_profile_edit.spec.ts",
    "test:generate-report": "npm run test && npx allure generate --clean -o allure-report",
    "testhead": "npx playwright test --headed --workers=1",
    "setup": "node scripts/setup-project.js",
    "validate": "node scripts/validate-setup.js",
    "test:client": "node scripts/run-client-tests.js",
    "test:client:all": "node scripts/run-client-tests.js --all",
    "test:client:full": "node scripts/run-client-tests.js --full",
    "test:client:minimal": "node scripts/run-client-tests.js --minimal",
    "test:client:validation": "node scripts/run-client-tests.js --validation",
    "test:client:persistence": "node scripts/run-client-tests.js --persistence",
    "test:client:help": "node scripts/run-client-tests.js --help",
    "test:client:edit": "node scripts/run-client-edit-tests.js",
    "test:client:delete": "node scripts/run-client-delete-tests.js",
    "test:client:edit:qa": "node scripts/run-client-edit-qa-tests.js",
    "test:enhanced": "node scripts/run-test-suite-enhanced.js",
    "test:enhanced:client": "node scripts/run-test-suite-enhanced.js --client",
    "test:enhanced:client:create": "node scripts/run-test-suite-enhanced.js --client:create",
    "test:enhanced:client:edit": "node scripts/run-test-suite-enhanced.js --client:edit",
    "test:enhanced:client:delete": "node scripts/run-test-suite-enhanced.js --client:delete",
    "test:enhanced:smoke": "node scripts/run-test-suite-enhanced.js --smoke",
    "test:enhanced:regression": "node scripts/run-test-suite-enhanced.js --regression",
    "test:enhanced:all": "node scripts/run-test-suite-enhanced.js --all",
    "test:client:enhanced": "node scripts/run-client-tests-enhanced.js",
    "test:client:edit:enhanced": "node scripts/run-client-edit-tests-enhanced.js",
    "test:client:delete:enhanced": "node scripts/run-client-delete-tests-enhanced.js",
    "test:comprehensive": "node scripts/run-comprehensive-tests-enhanced.js",
    "test:comprehensive:all": "node scripts/run-comprehensive-tests-enhanced.js --all",
    "test:comprehensive:client": "node scripts/run-comprehensive-tests-enhanced.js --client",
    "test:comprehensive:customer": "node scripts/run-comprehensive-tests-enhanced.js --customer",
    "test:comprehensive:facility": "node scripts/run-comprehensive-tests-enhanced.js --facility",
    "test:comprehensive:building": "node scripts/run-comprehensive-tests-enhanced.js --building",
    "test:comprehensive:system": "node scripts/run-comprehensive-tests-enhanced.js --system",
    "test:comprehensive:smoke": "node scripts/run-comprehensive-tests-enhanced.js --smoke",
    "test:comprehensive:regression": "node scripts/run-comprehensive-tests-enhanced.js --regression",
    "test:regression": "npx playwright test tests/regression/ --workers=1",
    "test:regression:smoke": "npx playwright test tests/regression/smoke/ --workers=1",
    "test:regression:functional": "npx playwright test tests/regression/functional/ --workers=1",
    "test:regression:integration": "npx playwright test tests/regression/integration/ --workers=1",
    "test:regression:ui": "npx playwright test tests/regression/ui/ --workers=1",
    "test:regression:performance": "npx playwright test tests/regression/performance/ --workers=1",
    "test:regression:security": "npx playwright test tests/regression/security/ --workers=1",
    "test:regression:all": "npx playwright test tests/regression/ --workers=1 --reporter=html",
    "test:regression:headed": "npx playwright test tests/regression/ --headed --workers=1",
    "test:trends:templates": "node scripts/run-trends-report-templates-tests.js",
    "test:trends:templates:headed": "npx playwright test tests/regression/functional/Trends_report_templates.spec.ts --headed --workers=1",
    "test:trends:templates:video": "npx playwright test tests/regression/functional/Trends_report_templates.spec.ts --video=retain-on-failure --workers=1",
    "test:report:draft": "node scripts/run-create-report-draft-tests.js",
    "test:report:draft:headed": "npx playwright test tests/regression/functional/Create_Report_Draft.spec.ts --headed --workers=1",
    "test:report:draft:video": "npx playwright test tests/regression/functional/Create_Report_Draft.spec.ts --video=retain-on-failure --workers=1",
    "allure:report": "allure generate allure-results --clean -o allure-report && allure open allure-report",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "netlify:deploy": "netlify deploy --prod --dir=allure-report",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:qa": "npx playwright test",
    "test:qa:headed": "npx playwright test --project=chromium-headed",
    "test:qa:headless": "npx playwright test --project=chromium-headless",
    "test:qa:both": "npx playwright test",
    "test:report": "allure serve allure-results",
    "test:open-report": "allure open allure-report",
    "netlify:build": "chmod +x scripts/netlify-build.sh && ./scripts/netlify-build.sh",
    "install:browsers": "playwright install",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "playwright",
    "typescript",
    "testing",
    "automation",
    "allure"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/mssql": "^9.1.7",
    "@types/node": "^22.19.1",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "allure-commandline": "^2.34.1",
    "allure-playwright": "^3.4.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "playwright": "^1.56.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "msnodesqlv8": "^4.5.0",
    "mssql": "^11.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
